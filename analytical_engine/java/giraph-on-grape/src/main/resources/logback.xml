    <logger name="com.alibaba.graphscope.parallel.netty.NettyServer" level="debug"/>
    <logger name="com.alibaba.graphscope.parallel.netty.handler.NettyServerHandler" level="debug"/>
    <logger name="com.alibaba.graphscope.parallel.netty.handler.NettyClientHandler" level="debug"/>
    <logger name="com.alibaba.graphscope.parallel.netty.request.impl.BatchWritableRequest" level="debug"/>
    <logger name="com.alibaba.graphscope.parallel.message.SimpleMessageStore" level="debug"/>
    <logger name="com.alibaba.graphscope.fragment.adaptor.ImmutableEdgecutFragmentAdaptor" level="error"/>
    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>


