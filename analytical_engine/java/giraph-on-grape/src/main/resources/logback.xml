    <logger name="com.alibaba.graphscope.app.GiraphComputationAdaptor" level="info"/>
    <logger name="org.apache.giraph.utils.ConfigurationUtils" level="info"/>
    <logger name="org.apache.giraph.conf.GiraphClasses" level="info"/>
    <logger name="org.apache.giraph.conf.GiraphTypes" level="info"/>
    <logger name="org.apache.giraph.graph.impl.VertexDataManagerImpl" level="info"/>
    <logger name="com.alibaba.graphscope.samples.MessageApp" level="info"/>
    <logger name="com.alibaba.graphscope.samples.MessageBenchMark" level="info"/>
    <logger name="com.alibaba.graphscope.samples.MaxApp" level="info"/>
    <logger name="com.alibaba.graphscope.samples.SSSP" level="error"/>
    <logger name="com.alibaba.graphscope.samples.PageRank" level="info"/>
    <logger name="com.alibaba.graphscope.factory.GiraphComputationFactory" level="info"/>
    <logger name="org.apache.giraph.graph.impl.AggregatorManagerImpl" level="error"/>
    <logger name="org.apache.giraph.graph.impl.AggregatorManagerNettyImpl" level="error"/>
    <logger name="org.apache.giraph.comm.requests.NettyMessageDecoder" level="error"/>
