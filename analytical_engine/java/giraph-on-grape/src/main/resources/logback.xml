<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
　　　　　 <encoder>
　　　　　　　　　<pattern>[%d{ISO8601}][%p][%t][%c:%L] %m%n</pattern>
　　　　　 </encoder>
　　　</appender>

    <logger name="com.alibaba.graphscope.stdcxx.FFIByteVector" level="error"/>
    <logger name="com.alibaba.graphscope.serialization.FFIByteVectorInputStream" level="error"/>
    <logger name="com.alibaba.graphscope.serialization.FFIByteVectorOutputStream" level="error"/>
    <logger name="com.alibaba.graphscope.parallel.impl.GiraphDefaultMessageManager" level="error"/>
    <logger name="com.alibaba.graphscope.app.GiraphComputationAdaptor" level="info"/>
    <logger name="org.apache.giraph.utils.ConfigurationUtils" level="info"/>
    <logger name="org.apache.giraph.conf.GiraphClasses" level="info"/>
    <logger name="org.apache.giraph.conf.GiraphTypes" level="info"/>
    <logger name="org.apache.giraph.graph.impl.VertexDataManagerImpl" level="info"/>
    <logger name="com.alibaba.graphscope.samples.MessageApp" level="info"/>
    <logger name="com.alibaba.graphscope.samples.MaxApp" level="info"/>
    <logger name="com.alibaba.graphscope.samples.SSSP" level="info"/>
    <logger name="com.alibaba.graphscope.samples.PageRank" level="info"/>
    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>


