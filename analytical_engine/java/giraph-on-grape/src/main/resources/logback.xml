<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
　　　　　 <encoder>
　　　　　　　　　<pattern>[%d{ISO8601}][%p][%t][%c:%L] %m%n</pattern>
　　　　　 </encoder>
　　　</appender>

    <logger name="com.alibaba.graphscope.stdcxx.FFIByteVector" level="error"/>
    <logger name="com.alibaba.graphscope.serialization.FFIByteVectorInputStream" level="error"/>
    <logger name="com.alibaba.graphscope.serialization.FFIByteVectorOutputStream" level="error"/>
